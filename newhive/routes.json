    "//": "-----------------------------------------",
    "//": "These routes are content domain",
    "//": "-----------------------------------------",

    "naked_expr_id": {
        "page_route": "/<expr_id>"
        ,"controller": "expr"
        ,"method": "fetch_naked"
        ,"content_domain": true
    }
    ,"naked_expr_name": {
        "page_route": "/<owner_name>/<path:expr_name>"
        ,"controller": "expr"
        ,"method": "fetch_naked"
        ,"content_domain": true
        ,"precedence": 0
    }
    ,"blank_sandbox": {
        "page_route": "/home/edit"
        ,"controller": "expr"
        ,"method": "editor_sandbox"
        ,"content_domain": true
        ,"precedence": 1
    }
    ,"editor_sandbox": {
        "page_route": "/home/edit/<expr_id>"
        ,"controller": "expr"
        ,"method": "editor_sandbox"
        ,"content_domain": true
        ,"precedence": 1
    }
    ,"expr_embed_id": {
        "page_route": "/e/<expr_id>"
        ,"api_route": "/api/expr/embed/<expr_id>"
        ,"controller": "expr"
        ,"method": "embed"
        ,"precedence": 1
    }
    ,"expr_embed": {
        "page_route": "/e/<owner_name>/<path:expr_name>"
        ,"api_route": "/api/expr/embed/<owner_name>/<path:expr_name>"
        ,"controller": "expr"
        ,"method": "embed"
        ,"precedence": 1
    }

    ,"//": "-----------------------------------------"
    ,"//": "These routes are community pages."
    ,"//": "-----------------------------------------"

    ,"home": {
        "page_route": "/"
        ,"api_route": "/api/feed/featured/"
        ,"controller": "community"
        ,"method": "hive_featured"
        ,"client_method": "home"
        ,"body_class": "nav"
        ,"require_login": true
    }
    ,"home_cat": {
        "page_route": "/home/cat/<tag_name>"
        ,"api_route": "/api/feed/cat/<tag_name>"
        ,"controller": "community"
        ,"method": "expressions_tag"
        ,"client_method": "cat"
        ,"_owner_name": "root"
        ,"body_class": "nav"
        ,"include_categories": true
    }
    ,"home_": {
        "page_route": "/home/cat"
        ,"api_route": "/api/feed/cat"
        ,"controller": "community"
        ,"method": "expressions_tag"
        ,"client_method": "cat"
        ,"_owner_name": "root"
        ,"body_class": "nav"
        ,"include_categories": true
    }
    ,"network": {
        "page_route": "/<username>:<any(Recent):special>"
        ,"api_route": "/api/feed/recent/<username>"
        ,"controller": "community"
        ,"method": "network_recent"
        ,"client_method": "grid"
        ,"body_class": "nav"
        ,"max_columns": 2
        ,"require_login": true
        ,"precedence": 1
    }

    ,"featured": {
        "page_route": "/home/featured"
        ,"api_route": "/api/feed/featured"
        ,"controller": "community"
        ,"method": "featured"
        ,"client_method": "profile"
        ,"body_class": "nav"
        ,"require_login": true
    }
    ,"recent": {
        "page_route": "/home/recent"
       ,"api_route": "/api/feed/recent"
       ,"controller": "community"
       ,"method": "recent"
       ,"client_method": "grid"
       ,"body_class": "nav"
       ,"max_columns": 2
       ,"require_login": true
    }
   ,"search": {
        "page_route": "/s"
